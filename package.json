{
  "name": "gatsby-source-contentstack",
  "version": "2.3.4",
  "description": "Gatsby source plugin for building websites using Contentstack as a data source",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir .",
    "watch": "babel -w src --out-dir ."
  },
  "main": "gatsby-node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/gatsby-source-contentstack"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": {
    "name": "Contentstack",
    "url": "https://www.contentstack.com"
  },
  "license": "MIT",
  "dependencies": {
    "gatsby-source-filesystem": "^3.7.1",
    "node-fetch": "2.6.1",
    "progress": "^2.0.3",
    "query-string": "7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "^2.3.1"
  }
}
